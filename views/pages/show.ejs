    <section id = "left">
        <div class="icons" id="weather">Weather
        </div>
        <section class="displayLeft">
                    <p>
                        <%= weatherArray[0].icon %>
                        <%= weatherArray[0].forecast %>
                    </p>

        </section>
        <div class="icons" id="temp">Forecast</div>
        <section class="displayLeft" id="weatherData">
            <% weatherArray.forEach(item => { %>
                    <h2 class="a">
                        <%= item.time %>
                    </h2>
                    <p class="b">
                        <%= item.averageTemp %>
                    </p>
                    <p class="c">
                        <%= item.icon %>
                    </p>
                    <% })%>
        </section>
    </section>
    <section id="right">
        <div class="icons" id="name">Good Morning Avrey</div>
        <div id="displayRight"></div>
        <div class="icons" >Local News</div>
        <section id="news" class="displayLeft"></section>
    </section>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script type="text/javascript" src="../app.js"></script>
</body>
